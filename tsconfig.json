{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "paths": {
      "@customerarea/*": ["./src/customerarea/*"],
      "@customer/*": ["./src/customerarea/customer/*"],
      "@notification/*": ["./src/customerarea/notification/*"],
      "@ixc/*": ["./src/ixc/*"],
      "@models/*": ["./src/models/*"],
    }
  }
}
